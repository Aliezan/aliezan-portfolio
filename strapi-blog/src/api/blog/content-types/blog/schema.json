{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blogs",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "previewImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "blogDescription": {
      "type": "text",
      "required": true
    },
    "blogTag": {
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    },
    "blogContentSection": {
      "type": "component",
      "repeatable": true,
      "component": "contents.content-section-markdown"
    }
  }
}
